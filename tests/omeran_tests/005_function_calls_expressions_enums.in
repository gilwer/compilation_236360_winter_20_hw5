// calling functions as expressions
enum days { SUN, MON, TUES };

enum ways { BUN, TON, FUSE };

int foo(enum days a) {
  return (int)a;
}

int shlomo(enum ways w) {
  return (int)w;
}

int kaki(int a, enum days bb, enum ways c) {
  int x = a + (int)bb + (int)c;
  return x*2;
}

